<!DOCTYPE html>
<html lang="en">

  <%- include("./partials/head.ejs") -%>


<body>

    <!--Header section-->
  <%- include("./partials/adminHeader.ejs") -%>

  <!-- ==============  all our content starts here  ============== -->

  <main id="main">
    <!-- =======Video Gallery Section ======= -->
    <section id="portfolio" class="portfolio">
      <div class="text-center pb-2">
        <h1>VIDEOS</h1>
      </div>
      <div class="container">
        <div class="row" data-aos="fade-up">
          <% for (const video of videos) {%>
          <div class="col-lg-4 col-md-6 col-12 portfolio-item filter-app">
            <iframe width="100%" height="232" src="<%= video.url %>" frameborder="0"
              allowfullscreen></iframe>    
          </div>
          <% } %>
    
        </div>
      </div>
    </section><!-- End Video gallery Section -->

    <!-- ======= Pictures Gallery Section ======= -->
    <section id="portfolio" class="portfolio">
      <div class="text-center pb-2">
        <h1>PICTURES</h1>
      </div>
      <div class="container">
        <div class="row portfolio-container" data-aos="fade-up">
          <% for (const image of images) { %>

          <div class="col-lg-4 col-md-6 portfolio-item filter-app">
            <div style="text-align: center;"><img src="<%= image.url %>" class="img-fluid" alt=""></div>
          </div>

          <% } %>
        </div>

       </div>
      </div>
    </section>
    <!-- End gallery Section -->
  </main>
    <!-- ==============  all our content ends here  ============== -->


        <div id="preloader"></div>
        <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i
            class="bi bi-arrow-up-short"></i></a>
            
        <%- include("./partials/scripts.ejs") -%>
    
</body>
    
</html>